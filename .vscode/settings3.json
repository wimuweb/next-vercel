 {
 // Overwrite
 "workbench.iconTheme": "material-icon-theme",
 "workbench.colorTheme": "codeSTACKr Theme",
 "editor.fontFamily": "'Fira Code'",
 "editor.fontSize": 16,
 "editor.renderWhitespace": "all",
 "editor.fontLigatures": true,
"editor.cursorBlinking": "phase",
"editor.minimap.renderCharacters": false,
"editor.minimap.maxColumn": 150,
"editor.minimap.showSlider": "always",
"editor.smoothScrolling": true,
"editor.cursorSmoothCaretAnimation": true,
 "window.zoomLevel": -1,


  Para que la fuente fira code muestre el
 estilo cursiva cuando corresponde
"editor.tokenColorCustomizations": {
  "Panda Syntax": {
    "textMateRules": \[
      {
        "scope": \[
          "comment",
          "punctuation.definition.comment",
          "comment.block",
          "comment.line",
          "comment.block.documentation",
          "comment.line.double-slash",
          "entity.other.attribute-name",
          "entity.other.attribute-name.html",
          "markup.italic",
          "entity.other.attribute-name.id",
          "entity.other.attribute-name.class.css",
          "entity.other.attribute-name.tag.jade",
          "entity.other.attribute-name.class.jade",
          "entity.other.attribute-name.id.jade",
          "keyword.control",
          "storage",
          "emphasis",
          "variable.language"
        \],
        "settings": {
          "fontStyle": "italic"
        }
      }
    \]
  }
},
"git.confirmSync"             : false,
"breadcrumbs.enabled"         : true,
"files.autoSave"              : "off",
"files.trimTrailingWhitespace": true,

// extensiones
"togglequotes.chars": ["\"", "'", "`", ""],
"print.fontSize": "8pt",
"eslint.alwaysShowStatus": true,
"editor.codeActionsOnSave": {
 "source.fixAll.eslint": true
},


"todohighlight.isCaseSensitive": true,
"colorHelper.disableGpu": 1,
"alignment.surroundSpace": {
 "colon": [
   0,
   1
 ],
 "assignment": [
   1,
   1
 ],
 "arrow": [
   1,
   1
 ],
 "comment": 1
}

 }